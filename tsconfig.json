{
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "listFiles": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitHelpers": false,
    "noImplicitAny": false,
    "noLib": false,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "outDir": "target"
  },
  "exclude": [
    "typings/brower.d.ts",
    "typings/**/*.ts"
  ]
}
