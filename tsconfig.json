{
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalAsyncFunctions": true,
    "experimentalDecorators": true,
    "listFiles": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitHelpers": false,
    "noImplicitAny": false,
    "noLib": false,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "typings/main",
    "typings/main.d.ts"
  ]
}
